{
  "productName": "ServiceNowAnywhere",
  "version": "0.0.0",
  "identifier": "net.mouries.dev",
  "app": {
    "macOSPrivateApi": true,
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "ServiceNow Anywhere",
        "label": "main",
        "width": 1400,
        "height": 1050,
        "decorations": true,
        "fullscreen": false,
        "resizable": true,
        "transparent": true,
        "titleBarStyle": "Overlay"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "build": {
    "frontendDist": "../dist/"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {}
}